<nav class="navbar navbar-expand-md fixed-top bg-primary navbar-light breadcrumb">
    {% for key, url in template.breadcrumbs %}

        {% if loop.last %}
            <li class="breadcrumb-item active">{{ trans(key) }}</li>
        {% else %}
            <li class="breadcrumb-item"><a href="{{ url }}">{{ trans(key) }}</a></li>
        {% endif %}

    {% endfor %}
</nav>
